<Window xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:fsxaml="http://github.com/fsprojects/FsXaml"
        xmlns:local="clr-namespace:ViewModels;assembly=MergePartition"
        MinHeight="220" MinWidth="300" Height="Auto"
        x:Name="Self">
    <Window.Resources>
        <fsxaml:BooleanToCollapsedConverter x:Key="TrueToCollapsed" />
    </Window.Resources>
    <Window.DataContext>
        <local:MainViewModel/>
    </Window.DataContext>
    <Grid >
        <TextBlock FontSize="72" Text="{Binding Title}" HorizontalAlignment="Center" VerticalAlignment="Center" />
    </Grid>
</Window>
